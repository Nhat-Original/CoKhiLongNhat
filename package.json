{
  "name": "next_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "npx husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev --skip-seed",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add ."
    ]
  },
  "dependencies": {
    "-": "^0.0.1",
    "@auth/prisma-adapter": "^1.0.11",
    "@prisma/client": "^5.10.2",
    "@tanstack/react-query": "^5.24.1",
    "@types/bcrypt": "^5.0.2",
    "autoprefixer": "^10.4.19",
    "bcrypt": "^5.1.1",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.7.8",
    "jose": "^5.6.3",
    "next": "14.2.22",
    "next-auth": "^4.24.5",
    "next-cloudinary": "^6.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^10.0.3",
    "save-dev": "^0.0.1-security",
    "universal-cookie": "^7.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
